{
  "name": "reactome-immport-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-deploy": "vue-cli-service build && cp -r dist/* tomcat/src/main/webapp",
    "build-nursa-deploy": "vue-cli-service build && rm -rf tomcat/src/main/webapp/* && cp -r dist/* tomcat/src/main/webapp && cd tomcat && mvn clean package && scp target/reactome-immport-web-1.0.0.war nursa:reactome-immport-web.war",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.8.2",
    "document-register-element": "^1.14.10",
    "lodash": "^4.17.20",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "vue-custom-element": "^3.2.14",
    "vue-cytoscape": "^1.0.8",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-service": "~4.3.0",
    "@vue/test-utils": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "friendly-errors-plugin": "^1.1.2",
    "jest": "^26.6.3",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
